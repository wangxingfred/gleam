pub fn return_tuple(x, y) {
  case x > y {
    True -> $return #(x, y)
    False -> #(y, x)
  }
}


----- COMPILED JAVASCRIPT
export function return_tuple(x, y) {
  let $ = x > y;
  if ($) {
    return [x, y];
  } else {
    return [y, x];
  }
}
