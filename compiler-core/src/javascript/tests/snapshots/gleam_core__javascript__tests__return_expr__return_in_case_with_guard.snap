pub fn return_with_guard(x) {
  case x {
    n if n > 0 -> $return n * 2
    _ -> 0
  }
}


----- COMPILED JAVASCRIPT
export function return_with_guard(x) {
  let n = x;
  if (n > 0) {
    return n * 2;
  } else {
    return 0;
  }
}
