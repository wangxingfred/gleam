----- SOURCE CODE

pub fn deeply_nested(x) {
  case x {
    0 -> {
      let y = 1
      case y {
        1 -> {
          let z = 2
          case z > 0 {
            True -> $return z * 10
            False -> z
          }
        }
        _ -> y
      }
    }
    _ -> x
  }
}


