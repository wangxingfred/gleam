----- SOURCE CODE

pub fn nested_case_return(x, y) {
  case x {
    0 -> case y {
      1 -> $return 42
      _ -> y + 1
    }
    _ -> x + y
  }
}


----- COMPILED JAVASCRIPT
export function nested_case_return(x, y) {
  if (x === 0) {
    if (y === 1) {
      return 42;
    } else {
      return y + 1;
    }
  } else {
    return x + y;
  }
}
