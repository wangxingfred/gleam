pub fn main(x: Int, y: Int, z: Int) -> Int {
  case x {
    1 -> case y {
      1 -> case z {
        1 -> $return 111
        2 -> $return 112
        _ -> 110
      }
      2 -> case z {
        1 -> $return 121
        _ -> 120
      }
      _ -> 100
    }
    2 -> case y {
      1 -> $return 210
      _ -> 200
    }
    _ -> 0
  }
}

