pub fn main(x: Int) -> Int {
  let step1 = x + 1
  case step1 < 0 {
    True -> $return -1
    False -> 0
  }

  let step2 = step1 * 2
  case step2 > 100 {
    True -> $return 100
    False -> 0
  }

  let step3 = step2 + 10
  case step3 > 50 {
    True -> $return step3
    False -> 0
  }

  step3 * 2
}

